@model Transporto.ViewModels.vehiculo.ListVehiculoViewModel
@{
    ViewBag.Title = "Información Soat";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Soat</h2>


<div class="col-md-6">
    <div class="card-box">
        <table class="table table-striped table-condensed">
            <thead>
                <tr>
                    <th>Placa</th>
                    <th>Soat Fecha De Inicio</th>
                    <th>Soat Fecha Vencimiento</th>
                    <th>Número Habilitación</th>
                </tr>
            </thead>
            <tbody>
            <tbody>
                @foreach (var item in Model.LstVehiculo)
                {
                    <tr>
                        <td> @item.Placa </td>
                        <td> @item.SoatFechaInic</td>
                        <td> @item.SoatFechVenc</td>
                        <td> @item.NumeroHabilitacion</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>