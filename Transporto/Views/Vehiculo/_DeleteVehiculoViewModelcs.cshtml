@model Transporto.ViewModels.vehiculo._DeleteVehiculoViewModelcs
@{ 
    ViewBag.Title = "Eliminar Vehiculo";
    Layout = ConstantHelpers.LAYOUT.MODAL_LAYOUT_PATH;
    ViewBag.Form = new ModalFormHelpers("DeleteVehiculo", "Vehiculo", FormMethod.Post);
}

<div class="row">
    <div class="col-sm-6">
        @Html.HiddenFor(x => x.VehiculoId)
        <h3>Esta seguro que desea eliminar este Vehiculo </h3>
    </div>
</div>
@section Footer {
    <button type="submit" class="btn btn-sm btn-primary"><i class="fa fa-check"></i> Aceptar</button>
}
