@model Transporto.ViewModels.vehiculo.ListVehiculoViewModel
@{
    ViewBag.Title = "Status Vehiculo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Estado Vehiculo</h2>

<div class="col-md-10">
    <div class="card-box">
        <table class="table table-striped table-condensed">
            <thead>
                <tr>
                    <th>Placa</th>
                    <th>Peso</th>
                    <th>Volumen</th>
                    <th>Fecha inicio revición tecnica</th>
                    <th>Fecha caducidad revición tecnica</th>
                    <th>Fecha inicio seguro vehicular</th>
                    <th>Fecha caducidad seguro vehicular</th>
                    <th>Numero de Habilitación</th>
                    <th>Fecha inicio certificado fumiga</th>
                    <th>Fecha caducidad certificado fumiga</th>
                    <th>Estado</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.LstVehiculo)
            {
                    <tr>
                        <td> @item.Placa </td>
                        <td> @item.Peso</td>
                        <td> @item.Volumen</td>
                        <td> @item.FechaDeInicioRevisionTecn</td>
                        <td> @item.FechaDeCaducidadReviciTecni</td>
                        <td> @item.FechaIncioSeguroVehicular</td>
                        <td> @item.FechaCaducidadSeguVehicu</td>
                        <td> @item.NumeroHabilitacion</td>
                        <td> @item.FechaInicioCertificadoFumiga</td>
                        <td> @item.FechaCaducidadCertificadoFumiga</td>
                        <td> @item.Estado</td>

                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>